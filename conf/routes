# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

# backend
GET     /dashboard                  controllers.backend.Application.dashboard()
GET     /dashboard/charts           controllers.backend.Application.charts()
GET     /dashboard/login            controllers.backend.Application.login()

# frontend
GET     /                           controllers.frontend.Application.index()


# Map static resources from the /public folder to the /assets URL path
GET     /assets/*file               controllers.Assets.versioned(path="/public", file: Asset)
